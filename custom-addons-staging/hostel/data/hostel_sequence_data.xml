<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="room_sequence_id" model="ir.sequence">
            <field name="name">Room Sequence</field>
            <field name="code">room_sequence_code</field>
            <field name="prefix">RM</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>

        </record>
    </data>
    <data noupdate="1">
        <record id="student_sequence_id" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">student_sequence_code</field>
            <field name="prefix">ST</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>

        </record>
    </data>

</odoo>
